<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
                                         "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
	<session-factory>
		<property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
		<property name="hibernate.connection.url">jdbc:mysql://localhost:3306/final</property>
		<property name="hibernate.connection.username">root</property>
		<property name="hibernate.connection.password"></property>
		
		<property name="hibernate.dialect">org.hibernate.dialect.MySQL5Dialect</property>
		<property name="hibernate.show_sql">true</property>

		<mapping class="com.myproject.pojo.RegisteredUser" />
		<mapping class="com.myproject.pojo.Address" />
		<mapping class="com.myproject.pojo.User" />
		<mapping class="com.myproject.pojo.Filename" />
		<mapping class="com.myproject.pojo.Retailer" />
		<mapping class="com.myproject.pojo.Request" />
		<mapping class="com.myproject.pojo.Payment" />
		<mapping class="com.myproject.pojo.Lottery" />
		<mapping class="com.myproject.pojo.WinningNumbers" />
		<mapping class="com.myproject.pojo.Category" />
		<mapping class="com.myproject.pojo.MillionsEveryday" />
		


	</session-factory>
</hibernate-configuration>